#include <iostream>

using namespace std;

int main()
{
    int a;
    float b;
    double c;
    long d;
    float s, v, e, f;
    cin >> a >> b >> c>> d;
    s = a + b;
    h = c / d;
    g = b * c;
    z = a - d;
    cout << "сумма = " << s << endl;
    cout << "деление = " << h << endl;
    cout << "произведение = " << g << endl;
    cout << "разность = " << z << endl;
    return 0;
}
